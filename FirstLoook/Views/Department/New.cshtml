
@{
    ViewData["Title"] = "New";
}
@model Department
<h1>New Department</h1>

<form method="post" action="/Department/Create">
 <div>
     <label>Name :</label>
     <input type="text" name="Name" id="Name" value="@Model.Name" class="form-check form-control"/>
 </div> 
 <div>
     <label>Manger Name :</label>
     <input type="text" name="Manger_Name" id="Manger_Name" value="@Model.Manger_Name" class="form-check form-control"/>
 </div> 
 
 <br />
 <input type="submit" value="Create" class="btn btn-outline-success"/>
</form>
 <br />
 <a href="/Department/GettAll" class="btn btn-primary">Back To List</a>
